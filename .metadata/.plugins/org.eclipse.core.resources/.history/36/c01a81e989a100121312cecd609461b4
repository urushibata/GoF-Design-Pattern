package command.drawer;

import java.awt.Point;
import command.cmmand.Command;

public class DrawCommand implements Command {

	// 描画対象
	protected Drawable drawable;
	// 描画位置
	private Point position;
	// コンストラクタ

	@Override
	public void execute(){
		drawable.draw(position.x,position.y);
	}

}
