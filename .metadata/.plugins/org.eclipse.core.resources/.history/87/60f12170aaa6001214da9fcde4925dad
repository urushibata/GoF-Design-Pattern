package singleton;

/**
 * @author urushibata
 * @version 1.0
 */
public class Singleton {
	private static Singleton singleton = new Singleton();
	private Singleton(){
		System.out.println("インスタンスを作成しました。");
	}

	/**
	 * @return singleton
	 * @since 1.0
	 */
	public static Singleton getInstance(){
		return singleton;
	}
}
