package adapter;

import adapter.transfer.Print;
import adapter.transfer.PrintBanner;
import adapter.inheritance.Print;
import adapter.inheritance.PrintBanner;

/**
 * Adapterパターンメインクラス
 * @author urushibata
 * @version 1.0
 */
public class Main {

	/**
	 * @param args
	 * @since 1.0
	 */
	public static void main(String[] args) {
		// 委譲パターン
		adapter.transfer.Print p = new adapter.transfer.PrintBanner("Hello");
		p.printWeak();
		p.printString();

		adapter.transfer.Print p = new adapter.transfer.PrintBanner("Hello");
		p.printWeak();
		p.printString();
	}
}