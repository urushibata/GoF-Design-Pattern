package adapter.transfer;

import adapter.transfer.Print;
import adapter.transfer.PrintBanner;

/**
 * Adapterパターン（継承）メインクラス
 * @author urushibata
 * @version 1.0
 */
public class Main {

	/**
	 * @param args
	 * @since 1.0
	 */
	public static void main(String[] args) {
		Print p = new PrintBanner("Hello");
		p.printWeak();
		p.printString();
	}
}